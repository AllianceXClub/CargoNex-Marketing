{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            {
                "framework": "vite",
                "buildCommand": "npm run build",
                "outputDirectory": "dist",
                "rewrites": [
                    {
                        "source": "/api/:path*",
                        "destination": "/api/:path*"
                    },
                    {
                        "source": "/(.*)",
                        "destination": "/index.html"
                    }
                ],
                "functions": {
                    "api/generate-marketing.ts": {
                        "maxDuration": 60
                    }
                }
            }